{
    "bazel.projectview.open": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/CVS": true,
        ".eclipse": false,
        ".git": true,
        "bazel-odp": false,
        "bazel-out": false,
        "bazel-bin": false,
        "bazel-testlogs": false,
        ".vscode": false,
        ".idea": false,
        ".bazelbsp": false,
    },
    "java.project.sourcePaths": [
        "java/src/it/java",
        "java/src/main/java",
        "java/src/test/java",
        "federatedcompute/fcp/java_src/main/java"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true,
        "**/!(.eclipse|java)/**": true
    },
    "java.project.referencedLibraries": [
        "bazel-out/k8-opt/bin/external/maven/v1/https/repo1.maven.org/maven2/**/*.jar",
        "bazel-bin/external/federatedcompute/fcp/protos/**/*.jar",
        "bazel-out/k8-opt/bin/external/**/*.jar",
        "bazel-out/k8-opt/bin/shuffler/**/*.jar",
        "bazel-bin/java/src/test/java/com/google/ondevicepersonalization/federatedcompute/shuffler/**/*.jar",
        "bazel-out/k8-opt/bin/external/federatedcompute/fcp/protos/**/*.jar",
    ],
    "java.import.bazel.disabled": false,
    "java.import.maven.enabled": false,
    "java.import.gradle.enabled": false,
    "java.configuration.updateBuildConfiguration": "disabled",
    "java.autobuild.enabled": false,
    "java.jdt.ls.vmargs": "-Xmx32G -XX:+UseParallelGC",
    "java.server.launchMode": "Hybrid",
    "java.import.gradle.wrapper.enabled": false,
    "java.import.gradle.annotationProcessing.enabled": false,
    "bazel.projectview.updateFileWatcherExclusion": false,
}