{
    "bazel.projectview.open": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/CVS": true,
        ".eclipse": false,
        ".git": false,
        "docs": false,
        "external": false,
        "federatedcompute": false,
        "java": false,
        "patches": false,
        "python": false,
        "scripts": false,
        "shuffler": false,
        "toolchain": false,
        "tools": false,
        "bazel-odp": false,
        "bazel-out": false,
        "bazel-bin": false,
        "bazel-testlogs": false,
        ".vscode": false,
        ".idea": false,
        ".bazelbsp": false
    },
    "java.project.sourcePaths": [
        "java/src/it/java",
        "java/src/main/java",
        "java/src/test/java"
    ],
    "java.project.referencedLibraries": [
        "bazel-out/k8-opt/bin/external/maven/v1/https/repo1.maven.org/maven2/**/*.jar",
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true
    },
    "java.import.bazel.disabled": false,
    "java.import.maven.enabled": false,
    "java.import.gradle.enabled": false
}